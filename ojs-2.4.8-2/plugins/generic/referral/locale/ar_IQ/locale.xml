	<message key="plugins.generic.referral.dateAdded">تأريخ إضافتها</message>
	<message key="plugins.generic.referral.all">الكل</message>
	<message key="plugins.generic.referral.all.empty">لا توجد روابط عائدة حالياً.</message>
	<message key="plugins.generic.referral.status.new">جديد</message>
	<message key="plugins.generic.referral.status.new.empty">لا توجد روابط عائدة جديدة حالياً.</message>
	<message key="plugins.generic.referral.status.accepted">منشور</message>
	<message key="plugins.generic.referral.status.accept">أنشر</message>
